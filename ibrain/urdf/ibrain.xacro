<?xml version="1.0" ?>

<robot name="ibrain" xmlns:xacro="https://www.ros.org/wiki/xacro" >


  <xacro:include filename="$(find ibrain)/urdf/_d435.urdf.xacro" />

  
  
  <gazebo reference="link_chassis">
    <material>Gazebo/Orange</material>
  </gazebo>

  <gazebo reference="link_left_wheel">
    <material>Gazebo/Blue</material>
  </gazebo>

  <gazebo reference="link_right_wheel">
    <material>Gazebo/Blue</material>
  </gazebo>

  <gazebo reference="link_right_wheel2">
    <material>Gazebo/Blue</material>
  </gazebo>

  <gazebo reference="link_left_wheel2">
    <material>Gazebo/Blue</material>
  </gazebo>  

   <gazebo reference="body">
		<material>Gazebo/DarkGrey</material>
	</gazebo>

   <gazebo reference="upper_body">
		<material>Gazebo/Black</material>
	</gazebo>

  <link name="link_chassis">
    <!-- pose and inertial -->
    <pose>0 0 0.1 0 0 0</pose>
    
    <inertial>
      <mass value="5"/>
      <origin rpy="0 0 0" xyz="0 0 0.1"/>
      <inertia ixx="0.0395416666667" ixy="0" ixz="0" iyy="0.106208333333" iyz="0" izz="0.106208333333"/>
    </inertial>
    
    <collision name="collision_chassis">
      <geometry>
        <box size="0.4 0.4 0.1"/>
      </geometry>
    </collision>
    
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.4 0.4 0.1"/>
      </geometry>
      <material name="blue"/>
    </visual>
  </link>
  
  <!-- Create wheel right -->  
    
  <link name="link_right_wheel">    
    <inertial>
      <mass value="0.2"/>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <inertia ixx="0.00052666666" ixy="0" ixz="0" iyy="0.00052666666" iyz="0" izz="0.001"/>
    </inertial>
    
    <collision name="link_right_wheel_collision">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
    </collision>
    
    <visual name="link_right_wheel_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>      
    </visual>
    
  </link>
    
  <!--   Joint for right wheel  -->
  <joint name="joint_right_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="-0.1 0.23 0"/>
    <child link="link_right_wheel" />
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0" />
  </joint>  
    
  
  <link name="link_right_wheel2">    
    <inertial>
      <mass value="0.2"/>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <inertia ixx="0.00052666666" ixy="0" ixz="0" iyy="0.00052666666" iyz="0" izz="0.001"/>
    </inertial>
    
    <collision name="link_right_wheel_collision">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
    </collision>
    
    <visual name="link_right_wheel_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>      
    </visual>
    
  </link>
    
  <!--   Joint for right wheel  -->
  <joint name="joint_right_wheel2" type="continuous">
    <origin rpy="0 0 0" xyz="0.1 0.23 0"/>
    <child link="link_right_wheel2" />
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0" />
  </joint>  
    
  <!-- Left Wheel link -->
    
  <link name="link_left_wheel">    
    <inertial>
      <mass value="0.2"/>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <inertia ixx="0.00052666666" ixy="0" ixz="0" iyy="0.00052666666" iyz="0" izz="0.001"/>
    </inertial>
    
    <collision name="link_left_wheel2_collision">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
    </collision>
    
    <visual name="link_left_wheel2_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>      
    </visual>
    
  </link>
    
  <!--   Joint for left wheel  -->
  <joint name="joint_left_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="-0.1 -0.23 0"/>
    <child link="link_left_wheel" />
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0" />
  </joint>    

    <!-- Left Wheel link 2-->
    
  <link name="link_left_wheel2">    
    <inertial>
      <mass value="0.2"/>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <inertia ixx="0.00052666666" ixy="0" ixz="0" iyy="0.00052666666" iyz="0" izz="0.001"/>
    </inertial>
    
    <collision name="link_left_wheel2_collision">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
    </collision>
    
    <visual name="link_left_wheel2_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>      
    </visual>
    
  </link>
    
  <!--   Joint for left wheel 2 -->
  <joint name="joint_left_wheel2" type="continuous">
    <origin rpy="0 0 0" xyz="0.1 -0.23 0"/>
    <child link="link_left_wheel2" />
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0" />
  </joint>    


 <!-- body -->
    <link name="body">
    <pose>0 0 0.1 0 0 0</pose>
    
    <inertial>
      <mass value="5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0395416666667" ixy="0" ixz="0" iyy="0.106208333333" iyz="0" izz="0.106208333333"/>
    </inertial>
    
    <collision name="collision_body">
      <geometry>
        <box size="0.2 0.2 1.1"/>
      </geometry>
    </collision>
    
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.07 0.2 1.1"/>
      </geometry>
    </visual>
  </link>
 
  
    <joint name="body_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.0 0.0 0.55"/>
    <child link="body" />
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="1 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0" />
  </joint>   

   <!-- upper_body -->
    <link name="upper_body">
    <pose>0 0 0.1 0 0 0</pose>
    
    <inertial>
      <mass value="5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0395416666667" ixy="0" ixz="0" iyy="0.106208333333" iyz="0" izz="0.106208333333"/>
    </inertial>
    
    <collision name="collision_upper_body">
      <geometry>
        <box size="0.5 0.5 0.7"/>
      </geometry>
    </collision>
    
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.5 0.7"/>
      </geometry>
      <material name="red"/>
    </visual>
  </link>
  
    <joint name="upper_body_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.0 0.0 1.1"/>
    <child link="upper_body" />
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="1 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0" />
  </joint>  



  <sensor_d435 parent="upper_body">
    <origin xyz="0.05 0.0 -0.36" rpy="0 0 0"/>
  </sensor_d435>


<!-- inner_body -->
    <link name="inner_body">
    <pose>0 0 0.1 0 0 0</pose>
    
    <inertial>
      <mass value="5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0395416666667" ixy="0" ixz="0" iyy="0.106208333333" iyz="0" izz="0.106208333333"/>
    </inertial>
    
    <collision name="collision_inner_body">
      <geometry>
        <box size="0.5 0.3 0.6"/>
      </geometry>
    </collision>
    
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.15 0.3 0.6"/>
      </geometry>
    </visual>
  </link>
  
    <joint name="inner_body_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.0 0.0 1.1"/>
    <child link="inner_body" />
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="1 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0" />
  </joint>  

  <!-- Lidar1 -->

   <link name="sensor_laser1">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1" />
      <!-- RANDOM INERTIA BELOW -->
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.02"/>
    </inertial>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.05" length="0.1"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.1"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_sensor_laser1" type="fixed">
    <origin xyz="0.15 0 0.1" rpy="0 0 0"/>
    <parent link="link_chassis"/>
    <child link="sensor_laser1"/>
  </joint>


	<gazebo>
		<plugin filename="libgazebo_ros_control.so" name="gazebo_ros_control">
			<robotNamespace>/ibrain</robotNamespace>
			<robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
			<legacyModeNS>true</legacyModeNS>
		</plugin>
	</gazebo>

	<gazebo>
		<plugin filename="libgazebo_ros_diff_drive.so" name="differential_drive_controller">
			<legacyMode>false</legacyMode>
			<alwaysOn>true</alwaysOn>
			<updateRate>20</updateRate>
			<leftJoint>joint_left_wheel</leftJoint>
      <leftJoint>joint_left_wheel2</leftJoint>
			<rightJoint>joint_right_wheel</rightJoint>
    	<rightJoint>joint_right_wheel2</rightJoint>
			<wheelSeparation>0.53</wheelSeparation>
			<wheelDiameter>0.1524</wheelDiameter>
			<commandTopic>cmd_vel</commandTopic>
			<odometryTopic>odom</odometryTopic>
			<odometryFrame>odom</odometryFrame>
			<robotBaseFrame>link_chassis</robotBaseFrame>
      <robotNamespace>/ibrain</robotNamespace>
		</plugin>
	</gazebo>

  	<!--Lidar1-->

	<gazebo reference="sensor_laser1">
		<sensor type="ray" name="head_hokuyo_sensor1">
			<pose>0 0 0 0 0 0</pose>
			<visualize>false</visualize>
			<update_rate>10</update_rate>
			<ray>
				<scan>
					<horizontal>
						<samples>720</samples>
						<resolution>1</resolution>
						<min_angle>-1.5</min_angle>
						<max_angle>1.5</max_angle>
					</horizontal>
				</scan>
				<range>
					<min>0.15</min>
					<max>8.0</max>
					<resolution>0.01</resolution>
				</range>
				<noise>
					<type>gaussian</type>
					<mean>0.0</mean>
					<stddev>0.01</stddev>
				</noise>
			</ray>
			<plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
				<topicName>/wh_base/laser/scan1</topicName>
				<frameName>sensor_laser1</frameName>
			</plugin>
		</sensor>
	</gazebo>


 



  

</robot>
